{
    "name": "kduma/sms-driver-serwersms",
    "description": "An SerwerSMS.pl driver for kduma/sms package",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Krystian Duma",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "illuminate/support": "^5.6",
        "serwersms/serwersms-php-client": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "KDuma\\SMS\\Drivers\\SerwerSMS\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KDuma\\SMS\\Drivers\\SerwerSMS\\SerwerSMSServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
